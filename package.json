  },
  "bin": {
    "vue-slint": "./build/index.js"
  },
  "keywords": [
    "slack",
    "vuecli",
    "lint",
    "eslint",
    "tslint"
  ],
  "author": {
    "name": "val antonini",
    "url": "https://valantonini.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/valantonini/vue-slint.git"
  },
  "license": "MIT",
  "dependencies": {
    "@slack/client": "^4.11.0",
    "@types/command-line-usage": "^5.0.1",
    "@types/node": "^11.11.1",
    "command-line-args": "^5.0.2",
    "command-line-usage": "^5.0.5",
    "shelljs": "^0.8.3"
  }
}
