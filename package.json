{
  "name": "vue-slint",
  "version": "0.0.8",
  "description": "Runs eslint/tslint on a git versioned VueCli project and send the results to slack tagging the last committer.",
  "homepage": "https://github.com/valantonini/vue-slint",
  "main": "./build/index.js",
  "scripts": {
    "build": "tsc",
    "exec": "node ./build/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
